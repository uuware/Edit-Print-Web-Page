{
  "name": "Edit&Print Web Page",
  "description": "Print Web Page but want remove some images? try this!",
  "version": "2.0",
  "manifest_version": 2,
  "homepage_url": "http://uuware.com/",
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "32": "icon-small.png",
    "48": "icon_48.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon-small.png"
  },
  "permissions": [
  "tabs", "\u003Call_urls\u003E", "http://*/*", "https://*/*"
  ]
}